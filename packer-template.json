{
  "builders":
  [
    {
      "type": "qemu",
      "headless": "true",
      "communicator": "ssh",
      "iso_url": "https://download.fedoraproject.org/pub/fedora/linux/releases/27/Server/x86_64/iso/Fedora-Server-netinst-x86_64-27-1.6.iso",
      "iso_checksum_url": "https://download.fedoraproject.org/pub/fedora/linux/releases/27/Server/x86_64/iso/Fedora-Server-27-1.6-x86_64-CHECKSUM",
      "iso_checksum_type": "sha256",
      "accelerator": "kvm",
      "disk_size": 30000,
      "ssh_username": "armet",
      "ssh_password": "123123",
      "ssh_port": 22,
      "ssh_timeout": "3000s",
      "output_directory": "armetos/qemu/",
      "http_directory": "kickstart/ks",
      "http_port_min": 10082,
      "http_port_max": 10089,
      "ssh_host_port_min": 2222,
      "ssh_host_port_max": 2229,
      "vm_name": "armetos-build",
      "vnc_bind_address": "0.0.0.0",
      "net_device": "virtio-net",
      "disk_interface": "virtio",
      "boot_wait": "5s",
      "boot_command":  [
	  "<tab><wait>",
          " ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/min.ks<enter>"
      ],
      "qemuargs": [
        [ "-m", "2048m" ]
      ]
    }
  ]
}
